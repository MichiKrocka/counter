TTY /dev/ttyUSB1
READ_TIME 1000
LOG_TYPE 1
LOG_FORMAT "%b %d %H:%M:%S Sensor %s C: %.2C F: %.2F"
CNT_FORMAT "%b %d %H:%M:%S Sensor %s #%n %C"
HUM_FORMAT "%b %d %H:%M:%S Sensor %s C: %.2C F: %.2F H: %h%%"
SENSORS 1
ROM 0 0x10 0xA4 0x41 0xD3 0x01 0x08 0x00 0x0B 
ROM 1 0x10 0x37 0x45 0xD3 0x01 0x08 0x00 0xFC 
